<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mysql.mapper.SingleTableMapper">

    <resultMap id="BaseResultMap" type="com.example.mysql.domain.SingleTable">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="key1" column="key1" jdbcType="VARCHAR"/>
            <result property="key2" column="key2" jdbcType="INTEGER"/>
            <result property="key3" column="key3" jdbcType="VARCHAR"/>
            <result property="keyPart1" column="key_part1" jdbcType="VARCHAR"/>
            <result property="keyPart2" column="key_part2" jdbcType="VARCHAR"/>
            <result property="keyPart3" column="key_part3" jdbcType="VARCHAR"/>
            <result property="commonField" column="common_field" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,key1,key2,
        key3,key_part1,key_part2,
        key_part3,common_field
    </sql>
</mapper>
